pipeline {
    agent { label (JAVA_17) }
    stages {
        stage(VCS) {
            steps {
                git url: 'https://github.com/avsrpavankalyan/spring-petclinic.git',
                    branch: 'build'
            }
        }
        stage(BUILD) {
            steps {
                sh './mvnw package'
            }
        }
    }
}