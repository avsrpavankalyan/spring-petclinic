pipeline {
    agent { label 'JAVA_17'}
    stages {
        stage(VCS) {
            steps {
                git url: 'https://github.com/avsrpavankalyan/spring-petclinic.git',
                branch: 'main'
            }
        }
        stage(PACKAGE) {
            steps {
                sh './mvnw package'
            }
        }
        stage(FACTS) {
            steps {
                archiveArtifacts archive: '**/target/spring-petclinic.jar'

            }

        }
    }
}